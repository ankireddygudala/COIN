<% layout('user/userLayout') -%>

    <div class="m-grid__item m-grid__item--fluid m-grid m-grid m-grid--hor m-container m-container--responsive m-container--xxl">
        <div class="m-grid__item m-grid__item--fluid m-grid m-grid--hor-desktop m-grid--desktop m-body">
            <div class="m-grid__item m-body__nav">
                <div class="m-stack m-stack--ver m-stack--desktop">
                    <!-- begin::Horizontal Menu -->
                    <div class="m-stack__item m-stack__item--middle m-stack__item--fluid">
                        <button class="m-aside-header-menu-mobile-close  m-aside-header-menu-mobile-close--skin-light " id="m_aside_header_menu_mobile_close_btn">
                            <i class="la la-close"></i>
                        </button>
                        <div id="m_header_menu" class="m-header-menu m-aside-header-menu-mobile m-aside-header-menu-mobile--offcanvas  m-header-menu--skin-light m-header-menu--submenu-skin-light m-aside-header-menu-mobile--skin-light m-aside-header-menu-mobile--submenu-skin-light ">
                            <ul class="m-menu__nav  m-menu__nav--submenu-arrow ">
                                <li class="m-menu__item " aria-haspopup="true">
                                    <a href="dashboard" class="m-menu__link ">
                                        <span class="m-menu__item-here"></span>
                                        <span class="m-menu__link-text">
                                            <%= i18n.__("DASHBOARD") %>
                                        </span>
                                    </a>
                                </li>
                                <!-- <li class="m-menu__item" aria-haspopup="true">
                                    <a href="leads" class="m-menu__link">
                                        <span class="m-menu__item-here"></span>
                                        <span class="m-menu__link-text">
                                            <%= i18n.__("LEADS")%>
                                        </span>
                                    </a>
                                </li> -->
                                <li class="m-menu__item" aria-haspopup="true">
                                    <a href="transactions" class="m-menu__link">
                                        <span class="m-menu__item-here"></span>
                                        <span class="m-menu__link-text">
                                            <%= i18n.__("TRANSACTIONS") %>
                                        </span>
                                        <!-- <i class="m-menu__hor-arrow la la-angle-down"></i>
                                        <i class="m-menu__ver-arrow la la-angle-right"></i> -->
                                    </a>
                                </li>
                                <li class="m-menu__item" aria-haspopup="true">
                                    <a href="referral_links" class="m-menu__link">
                                        <span class="m-menu__item-here"></span>
                                        <span class="m-menu__link-text">
                                            <%= i18n.__("REFERRAL_LINKS") %>
                                        </span>
                                        <!-- <i class="m-menu__hor-arrow la la-angle-down"></i>
                                        <i class="m-menu__ver-arrow la la-angle-right"></i> -->
                                    </a>
                                </li>
                                <li class="m-menu__item m-menu__item--active" aria-haspopup="true">
                                    <a href="referral_traffic" class="m-menu__link">
                                        <span class="m-menu__item-here"></span>
                                        <span class="m-menu__link-text">
                                            <%= i18n.__("REFERRAL_TRAFFIC") %>
                                        </span>
                                        <!-- <i class="m-menu__hor-arrow la la-angle-down"></i>
                                        <i class="m-menu__ver-arrow la la-angle-right"></i> -->
                                    </a>
                                </li>
                                <li class="m-menu__item" aria-haspopup="true">
                                    <a href="payouts" class="m-menu__link">
                                        <span class="m-menu__item-here"></span>
                                        <span class="m-menu__link-text">
                                            <%= i18n.__("PAYOUTS") %>
                                        </span>
                                        <!-- <i class="m-menu__hor-arrow la la-angle-down"></i>
                                        <i class="m-menu__ver-arrow la la-angle-right"></i> -->
                                    </a>
                                </li>

                            </ul>
                        </div>
                    </div>
                    <!-- end::Horizontal Menu -->
                </div>
            </div>
            <!--============================== insert submenu code here================-->

            <div class="m-grid__item m-grid__item--fluid m-grid m-grid--desktop m-grid--ver-desktop m-body__content">
                <div class="m-grid__item m-grid__item--fluid m-wrapper">
                    <!-- BEGIN: Subheader -->
                    <div class="m-subheader ">
                        <div class="d-flex align-items-center">
                            <div class="mr-auto">

                            </div>
                            <div>
                            </div>
                        </div>
                    </div>
                    <!-- END: Subheader -->
                    <div class="m-content">

                        <!-- top level1 -->
                        <div class="m-portlet ">
                            <div class="m-portlet__body  m-portlet__body--no-padding">
                                <div class="row m-row--no-padding m-row--col-separator-xl" style="height:90px;padding-top:10px">
                                    <div class="col-lg-6 col-md-6 col-xl-6 col">
                                        <!--begin::Total Profit-->
                                        <div class="m-widget24">
                                            <div class="m-widget24__item">
                                                    <div class="row m-row--no-padding">
                                                        <div class="col-md-4 col-sm-4 col-xl-4 col">
                                                            <div class="apro_user" align="center">
                                                                <img src="/assets/img/rocket.svg" alt="" class="img-responsive">
                                                            </div>
                                                        </div>
                                                        <div class="col-md-8 col-sm-8 col-xl-8 col" style="border: 0px solid red;">
                                                            <div class="apro_text">
                                                                <h2>0</h2>
                                                                <h4>
                                                                    <%= i18n.__("VISITORS") %>
                                                                        <span class="small-text">(
                                                                            <%= i18n.__("SELECTED_PERIOD") %>)</span>
                                                                </h4>
                                                            </div>
                                                        </div>
                                                    </div>
                                            
                                            </div>
                                        </div>
                                        <!--end::Total Profit-->
                                    </div>
                                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                        <!--begin::New Feedbacks-->
                                        <div class="m-widget24">
                                            <div class="m-widget24__item">
                                            
                                                    <div class="row m-row--no-padding">
                                                        <div class="col-md-4 col-sm-4 col-xl-4 col">
                                                            <div class="apro_user" align="center">
                                                                <img src="/assets/img/rightmark.svg" alt="" class="img-responsive">
                                                            </div>
                                                        </div>
                                                        <div class="col-md-8 col-sm-8 col-xl-8 col">
                                                            <div class="apro_text">
                                                                <h2>0.00%</h2>
                                                                <h4>
                                                                    <%= i18n.__("CONVERSION_RATE") %>
                                                                        <span class="small-text">(
                                                                            <%= i18n.__("SELECTED_PERIOD") %>)</span>
                                                                </h4>
                                                            </div>
                                                        </div>
                                                    </div>
                                           
                                            </div>
                                        </div>
                                        <!--end::New Feedbacks-->
                                    </div>

                                </div>
                            </div>
                        </div>
                        <!-- top levele1 end -->
                        <!-- table last -->

                    </div>
                    <div class="m-portlet">
                        <div class="m-portlet__body  m-portlet__body--no-padding">
                            <div class="row m-row--no-padding m-row--col-separator-xl">
                                <div class="col-xl-12">
                                    <div class="m-portlet m-portlet--brand m-portlet--head-solid-bg">
                                        <div class="m-portlet__head">
                                            <div class="m-portlet__head-caption">
                                                <div class="m-portlet__head-title">
                                                    <h3 class="m-portlet__head-text">
                                                        <%= i18n.__("ALL_REFERRAL_TRAFFIC") %>
                                                    </h3>
                                                </div>
                                            </div>
                                            
                                        </div>
                                        <div class="m-portlet__body">
                                            <!--begin: Datatable -->
                                            <div id="status"></div>
                                            <div id="users_wrapper" class="dataTables_wrapper no-footer">
                                                
                                               
                                                <table id="users" class="display dataTable no-footer" style="width: 100%;" role="grid" aria-describedby="users_info">
                                                    <thead>
                                                        <tr role="row">
                                                            <th class="sorting_asc" tabindex="0" aria-controls="users" rowspan="1" colspan="1" style="width: 74px;" aria-sort="ascending"
                                                                aria-label="IP Address: activate to sort column descending">
                                                                <%= i18n.__("IP_ADDRESS") %>
                                                            </th>
                                                            <th class="sorting" tabindex="0" aria-controls="users" rowspan="1" colspan="1" style="width: 57px;" aria-label="Browser: activate to sort column ascending">
                                                                <%= i18n.__("BROWSER") %>
                                                            </th>
                                                            <th class="sorting" tabindex="0" aria-controls="users" rowspan="1" colspan="1" style="width: 20px;" aria-label="OS: activate to sort column ascending">
                                                                <%= i18n.__("OS") %>
                                                            </th>
                                                            <th class="sorting" tabindex="0" aria-controls="users" rowspan="1" colspan="1" style="width: 56px;" aria-label="Country: activate to sort column ascending">
                                                                <%= i18n.__("COUNTRY") %>
                                                            </th>
                                                            <th class="sorting" tabindex="0" aria-controls="users" rowspan="1" colspan="1" style="width: 97px;" aria-label="Landing Page: activate to sort column ascending">
                                                                <%= i18n.__("LANDING_PAGE") %>
                                                            </th>
                                                            <th class="sorting" tabindex="0" aria-controls="users" rowspan="1" colspan="1" style="width: 81px;" aria-label="Date / Time: activate to sort column ascending">
                                                                <%= i18n.__("DATETIME") %>
                                                            </th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr class="odd">
                                                            <td valign="top" colspan="6" class="dataTables_empty">
                                                                <%= i18n.__("NO_DATA_IN_TABLE") %>
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                                
                                            </div>
                                            <!--end: Datatable -->
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>