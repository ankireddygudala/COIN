                        <div class="m-portlet__body">
                            <!--begin: Datatable -->
                            <div id="status"></div>
                            <div id="users_wrapper" class="dataTables_wrapper no-footer">
                                <div class="dataTables_length" id="users_length">
                                    <label>
                                        <%= i18n.__("SHOW") %>
                                            <select name="users_length" aria-controls="users" class="">
                                                <option value="10">10</option>
                                                <option value="25">25</option>
                                                <option value="50">50</option>
                                                <option value="100">100</option>
                                            </select>
                                            <%= i18n.__("ENTRIES") %>
                                    </label>
                                </div>
                                <div id="users_filter" class="dataTables_filter">
                                    <label>
                                        <%= i18n.__("SEARCH") %> :
                                            <input type="search" class="" placeholder="<%= i18n.__('SEARCH') %>..." aria-controls="users">
                                    </label>
                                </div>
                                <table class="display dataTable no-footer" id="users" width="100%" role="grid" aria-describedby="users_info" style="width: 100%;">
                                    <thead>
                                        <tr role="row">
                                            <th class="sorting_asc" tabindex="0" aria-controls="users" rowspan="1" colspan="1" style="width: 76px;" aria-sort="ascending"
                                                aria-label="Lead ID: activate to sort column descending">
                                                <%= i18n.__("LEAD_ID") %>
                                            </th>
                                            <th class="sorting" tabindex="0" aria-controls="users" rowspan="1" colspan="1" style="width: 161px;" aria-label="Affiliate Earnings: activate to sort column ascending">
                                                <%= i18n.__("AFFILIATE_LEAD_EARNINGS") %>
                                            </th>
                                            <th class="sorting" tabindex="0" aria-controls="users" rowspan="1" colspan="1" style="width: 108px;" aria-label="Conversion: activate to sort column ascending">
                                                <%= i18n.__("CONVERSION_RATE") %>
                                            </th>
                                            <th class="sorting" tabindex="0" aria-controls="users" rowspan="1" colspan="1" style="width: 112px;" aria-label="Date / Time: activate to sort column ascending">
                                                <%= i18n.__("DATETIME") %>
                                            </th>
                                        </tr>
                                    </thead>
                                    <tbody>

                                        <tr class="m-datatable__row--even">
                                            <td valign="top" colspan="4" class="dataTables_empty">
                                                <%= i18n.__("NO_DATA_IN_TABLE") %>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
